{
  "name": "SimpleQueryBuilder",
  "description": "Docker project for console php 7.4 projects with composer and phpunit",
  "authors": [
    {
      "name": "Aleksandr Kravchuk",
      "email": "aleksandr.kravchuk@ukr.net"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "MySimpleQueryBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SampleTest\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "phpstan/phpstan": "^0.12.18",
    "friendsofphp/php-cs-fixer": "^2.16"
  }
}
